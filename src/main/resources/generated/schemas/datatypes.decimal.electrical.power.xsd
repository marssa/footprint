<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<xs:schema version="1.0" targetNamespace="org.marssa.footprint.datatypes.decimal.electrical.power" xmlns:ns1="org.marssa.footprint.datatypes.decimal" xmlns:tns="org.marssa.footprint.datatypes.decimal.electrical.power" xmlns:xs="http://www.w3.org/2001/XMLSchema">

  <xs:import namespace="org.marssa.footprint.datatypes.decimal" schemaLocation="datatypes.decimal.xsd"/>

  <xs:complexType name="APower" abstract="true">
    <xs:simpleContent>
      <xs:extension base="ns1:MDecimal"/>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="KWatts">
    <xs:simpleContent>
      <xs:extension base="tns:APower"/>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="MWatts">
    <xs:simpleContent>
      <xs:extension base="tns:APower"/>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="Watts">
    <xs:simpleContent>
      <xs:extension base="tns:APower"/>
    </xs:simpleContent>
  </xs:complexType>
</xs:schema>

